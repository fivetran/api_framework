  "connector_list": "criterion_bail",
  "preprocessing_sql": "UPDATE {table_list} SET _fivetran_deleted = True WHERE connector_id IN ({connector_list}) AND last_sync > '{CUSTOMERS_last_sync}'",

  "use_privatelink": "false",
  "privatelink_host": ""
